<view><view class="auth"><view class="auth-title">输入短信验证码</view><view class="auth-mobile">短信验证码至<text>{{mobile}}</text></view><view class="codes"><block wx:for="{{config.count}}" wx:for-item="item" wx:for-index="__i0__" wx:key="key"><view><view data-event-opts="{{[['tap',[['hanldeOpenKeyboard',['$event']]]]]}}" class="{{['input',(currentFocus==item.index)?'active':'']}}" bindtap="__e"><block wx:if="{{code[item.index-1]!=undefined}}">{{''+code[item.index-1]+''}}</block><block wx:else><block wx:if="{{currentFocus==item.index}}"><view class="shining"></view></block></block></view></view></block></view><view class="auth-again"><block wx:if="{{countdown>0}}"><text class="time">{{countdown+"秒后重新发送"}}</text></block><block wx:if="{{countdown==0}}"><text data-event-opts="{{[['tap',[['resend',['$event']]]]]}}" bindtap="__e">重新发送</text></block><block wx:if="{{countdown<50}}"><text>没有收到验证码？</text></block></view></view><wanl-keyboard vue-id="7cbb4f89-1" open="{{keyboardVisible}}" data-event-opts="{{[['^number',[['inputCode']]],['^delete',[['deleteCode']]],['^empty',[['emptyCode']]],['^close',[['e0']]]]}}" bind:number="__e" bind:delete="__e" bind:empty="__e" bind:close="__e" bind:__l="__l"></wanl-keyboard></view>