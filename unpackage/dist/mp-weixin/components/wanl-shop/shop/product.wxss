.flbooth-product-container{background-color:#f9f9f9;border-radius:30rpx 30rpx 0 0}.flbooth-product-container__bar{display:flex;align-items:center;justify-content:space-between;padding:0 30rpx;height:40px;overflow:hidden;color:#555}.flbooth-product-container__bar .list{width:75%;display:flex;align-items:center;justify-content:space-between}.flbooth-product-container__bar .list .item{display:flex;align-items:center}.flbooth-product-container__bar .list .item.active{color:#f40;font-weight:700}.flbooth-product-container__bar .list .item .box{color:#ccc;margin-top:2rpx;margin-left:8rpx;display:flex;flex-direction:column;line-height:1}.flbooth-product-container__bar .list .item .box .active{color:#f40}